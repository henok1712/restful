pipeline{
agent any
stages {
    stage ('Build Application'){
        steps {
        
            bat 'mvn clean install'
        }
    }

    stage('Deploy Application Mulesoft Cloudhub'){
        steps {
    
                   bat 'mvn clean package deploy -Dmule.version=4.2.2 -Danypoint.username=Nordic123 -Danypoint.password=Nordic123 -Denvironment=Sandbox -Dworkers=1 -Dcloudhub.workerType=Micro -Dapplication.name=restfulProject '
           
            }
        }
     }
 }
